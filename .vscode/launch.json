  "version": "0.2.0",
  "configurations": [
    {
      "name": "Laravel Artisan Serve",
      "type": "php",
      "request": "launch",
      "program": "${workspaceFolder}/artisan",
      "args": ["serve"],
      "cwd": "${workspaceFolder}",
      "port": 9003,
      "env": {
        "XDEBUG_MODE": "debug",
        "XDEBUG_TRIGGER": "1"
      }
    }
  ]
}
